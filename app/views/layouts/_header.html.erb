<div class="container-fluid">
  <div class="row header">
    <div class="col-4 col-sm-4 col-md-5">
    </div>
    <div class="col-3 col-sm-4 col-md-4">
    </div>
    <div class="col-5 col-sm-4 col-md-3 text-center my-auto align-self-end">
      <% if current_user.nil? %>
       <%= link_to new_user_session_path, class: "btn btn-lg login-button" do %>Log In<% end %>
      <% else %>
       <%= link_to user_root_path, class: "btn btn-lg regular-button" do %>Go to App<% end %>
       <%= link_to destroy_user_session_path, class: "btn btn-lg regular-button", method: :delete do %>Log out<% end %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="header-logo">
      <%= link_to image_tag("/images/mob_earth_circle.svg"), root_path %>
    </div>
  </div>
</div>
